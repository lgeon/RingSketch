/* -*- P4_16 -*- */

#include <core.p4>
#include <tna.p4>

/*************************************************************************
 ************* C O N S T A N T S    A N D   T Y P E S  *******************
**************************************************************************/

#define IPV4        0x0800 // ETHERTYPE_IPV4
#define UDP         0x11  // PROTO_UDP
#define TCP         0x06  // PROTO_TCP

/*************************************************************************
 ***********************  H E A D E R S  *********************************
 *************************************************************************/

/*  Define all the headers the program will recognize             */
/*  The actual sets of headers processed by each gress can differ */

/* Standard ethernet header */
header ethernet_h {
    bit<48>   dst_addr;
    bit<48>   src_addr;
    bit<16>   ether_type;
}

header ipv4_h {
    bit<4>   version;
    bit<4>   ihl;
    bit<8>   diffserv;
    bit<16>  total_len;
    bit<16>  identification;
    bit<3>   flags;
    bit<13>  frag_offset;
    bit<8>   ttl;
    bit<8>   protocol;
    bit<16>  hdr_checksum;
    bit<32>  src_addr;
    bit<32>  dst_addr;
}

header l4port_h {
    bit<16> src_port;
    bit<16> dst_port;
}

header tcp_h {
    bit<32>  seq_no;
    bit<32>  ack_no;
    bit<4>   data_offset;
    bit<4>   res;
    bit<8>   flags;
    bit<16>  window;
    bit<16>  checksum;
    bit<16>  urgent_ptr;
}

header udp_h {
    bit<16>  len;
    bit<16>  checksum;
}

struct my_ingress_headers_t {
    ethernet_h   ethernet;
    ipv4_h       ipv4;
    l4port_h     ports;
    tcp_h        tcp;
    udp_h        udp;
}

struct my_ingress_metadata_t {
    bit<32> index_sketch1;
    bit<32> index_sketch2;
    bit<32> index_sketch3;
}

/* -*- P4_16 -*- */

/*************************************************************************
 **************  I N G R E S S   P R O C E S S I N G   *******************
 *************************************************************************/

    /***********************  P A R S E R  **************************/
parser IngressParser(packet_in        pkt,
    /* User */    
    out my_ingress_headers_t          hdr,
    out my_ingress_metadata_t         meta,
    /* Intrinsic */
    out ingress_intrinsic_metadata_t  ig_intr_md)
{
    /* This is a mandatory state, required by Tofino Architecture */
    state start {
        pkt.extract(ig_intr_md);
        pkt.advance(PORT_METADATA_SIZE);
        transition init_meta;
    }

    state init_meta {
        meta = { 0, 0, 0};
        transition parse_ethernet;
    }

    state parse_ethernet {
        pkt.extract(hdr.ethernet);
        transition select(hdr.ethernet.ether_type) {
            IPV4 :  parse_ipv4;
            default: reject;
        }
    }

    state parse_ipv4 {
        pkt.extract(hdr.ipv4);
        transition select(hdr.ipv4.frag_offset, hdr.ipv4.protocol) {
            ( 0, TCP  ) : parse_tcp;
            ( 0, UDP  ) : parse_udp;
            default : reject;
        }
    }

    state parse_tcp {
        pkt.extract(hdr.ports);
        pkt.extract(hdr.tcp);
        transition accept;
    }
    
    state parse_udp {
        pkt.extract(hdr.ports);
        pkt.extract(hdr.udp);
        transition accept;
    }

}
/*************************************************************************
 **************  I N G R E S S   P R O C E S S I N G   *******************
 *************************************************************************/

    /***************** M A T C H - A C T I O N  *********************/

control Ingress(
    /* User */
    inout my_ingress_headers_t hdr,
    inout my_ingress_metadata_t meta,
    /* Intrinsic */
    in ingress_intrinsic_metadata_t ig_intr_md,
    in ingress_intrinsic_metadata_from_parser_t ig_prsr_md,
    inout ingress_intrinsic_metadata_for_deparser_t ig_dprsr_md,
    inout ingress_intrinsic_metadata_for_tm_t ig_tm_md)
{
    CRCPolynomial<bit<32>>(
        coeff    = 0x04C11DB7,
        reversed = true,
        msb      = false,
        extended = false,
        init     = 0xFFFFFFFF,
        xor      = 0xFFFFFFFF) poly1;
    Hash<bit<32>>(HashAlgorithm_t.CUSTOM, poly1) hash_algo1;

    CRCPolynomial<bit<32>>(
        coeff    = 0x741B8CD7,
        reversed = true,
        msb      = false,
        extended = false,
        init     = 0xFFFFFFFF,
        xor      = 0xFFFFFFFF) poly2;
    Hash<bit<32>>(HashAlgorithm_t.CUSTOM, poly2) hash_algo2;

    CRCPolynomial<bit<32>>(
        coeff    = 0xDB710641,
        reversed = true,
        msb      = false,
        extended = false,
        init     = 0xFFFFFFFF,
        xor      = 0xFFFFFFFF) poly3;
    Hash<bit<32>>(HashAlgorithm_t.CUSTOM, poly3) hash_algo3;

    action do_hash1() {
        meta.index_sketch1 = hash_algo1.get({
                hdr.ipv4.src_addr,
                hdr.ipv4.dst_addr,
                hdr.ipv4.protocol,
                hdr.ports.src_port,
                hdr.ports.dst_port
            });
    }

    action do_hash2() {
        meta.index_sketch2 = hash_algo2.get({
                hdr.ipv4.src_addr,
                hdr.ipv4.dst_addr,
                hdr.ipv4.protocol,
                hdr.ports.src_port,
                hdr.ports.dst_port
            });
    }

    action do_hash3() {
        meta.index_sketch3 = hash_algo3.get({
                hdr.ipv4.src_addr,
                hdr.ipv4.dst_addr,
                hdr.ipv4.protocol,
                hdr.ports.src_port,
                hdr.ports.dst_port
            });
    }

    action send(PortId_t port) {
        ig_tm_md.ucast_egress_port = port;
    }

    action drop() {
        ig_dprsr_md.drop_ctl = 1;
    }

    table simple_fwd {
        key = {
            ig_intr_md.ingress_port : exact;
        }
        actions = {
            send; drop; NoAction;
        }
        const default_action = NoAction();
        size = 64;
    }

    Register<bit<32>, _>(1,0x1FFFF) N;

    RegisterAction<bit<32>, _, bit<32>>(N)
    getSub = {
        void apply(inout bit<32> register_data, out bit<32> result) {
            if(register_data == 0x1FFFF){
                register_data = 0x0000;
            }else{
                register_data = register_data + 1;
            }
            result = register_data;
        }
    };

    Register<bit<8>, _>(32768) flag11;
    Register<bit<8>, _>(32768) flag12;
    Register<bit<8>, _>(32768) flag13;

    RegisterAction<bit<8>, _, bit<1>>(flag11)
    setFlag11 = {
        void apply(inout bit<8> register_data, out bit<1> result) {
            if(register_data == 0x1){
                register_data = 0x0;
                result = 0x1;
            }else{
                result = 0x0;
            }
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag11)
    revFlag11 = {
        void apply(inout bit<8> register_data) {
            register_data = 0x1;
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag12)
    setFlag12 = {
        void apply(inout bit<8> register_data, out bit<1> result) {
            if(register_data == 0x1){
                register_data = 0x0;
                result = 0x1;
            }else{
                result = 0x0;
            }
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag12)
    revFlag12 = {
        void apply(inout bit<8> register_data) {
            register_data = 0x1;
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag13)
    setFlag13 = {
        void apply(inout bit<8> register_data, out bit<1> result) {
            if(register_data == 0x1){
                register_data = 0x0;
                result = 0x1;
            }else{
                result = 0x0;
            }
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag13)
    revFlag13 = {
        void apply(inout bit<8> register_data) {
            register_data = 0x1;
        }
    };

    Register<bit<8>, _>(32768) flag21;
    Register<bit<8>, _>(32768) flag22;
    Register<bit<8>, _>(32768) flag23;

    RegisterAction<bit<8>, _, bit<1>>(flag21)
    setFlag21 = {
        void apply(inout bit<8> register_data, out bit<1> result) {
            if(register_data == 0x1){
                register_data = 0x0;
                result = 0x1;
            }else{
                result = 0x0;
            }
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag21)
    revFlag21 = {
        void apply(inout bit<8> register_data) {
            register_data = 0x1;
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag22)
    setFlag22 = {
        void apply(inout bit<8> register_data, out bit<1> result) {
            if(register_data == 0x1){
                register_data = 0x0;
                result = 0x1;
            }else{
                result = 0x0;
            }
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag22)
    revFlag22 = {
        void apply(inout bit<8> register_data) {
            register_data = 0x1;
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag23)
    setFlag23 = {
        void apply(inout bit<8> register_data, out bit<1> result) {
            if(register_data == 0x1){
                register_data = 0x0;
                result = 0x1;
            }else{
                result = 0x0;
            }
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag23)
    revFlag23 = {
        void apply(inout bit<8> register_data) {
            register_data = 0x1;
        }
    };

    Register<bit<8>, _>(32768) flag31;
    Register<bit<8>, _>(32768) flag32;
    Register<bit<8>, _>(32768) flag33;

    RegisterAction<bit<8>, _, bit<1>>(flag31)
    setFlag31 = {
        void apply(inout bit<8> register_data, out bit<1> result) {
            if(register_data == 0x1){
                register_data = 0x0;
                result = 0x1;
            }else{
                result = 0x0;
            }
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag31)
    revFlag31 = {
        void apply(inout bit<8> register_data) {
            register_data = 0x1;
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag32)
    setFlag32 = {
        void apply(inout bit<8> register_data, out bit<1> result) {
            if(register_data == 0x1){
                register_data = 0x0;
                result = 0x1;
            }else{
                result = 0x0;
            }
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag32)
    revFlag32 = {
        void apply(inout bit<8> register_data) {
            register_data = 0x1;
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag33)
    setFlag33 = {
        void apply(inout bit<8> register_data, out bit<1> result) {
            if(register_data == 0x1){
                register_data = 0x0;
                result = 0x1;
            }else{
                result = 0x0;
            }
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag33)
    revFlag33 = {
        void apply(inout bit<8> register_data) {
            register_data = 0x1;
        }
    };

    Register<bit<8>, _>(32768) flag41;
    Register<bit<8>, _>(32768) flag42;
    Register<bit<8>, _>(32768) flag43;

    RegisterAction<bit<8>, _, bit<1>>(flag41)
    setFlag41 = {
        void apply(inout bit<8> register_data, out bit<1> result) {
            if(register_data == 0x1){
                register_data = 0x0;
                result = 0x1;
            }else{
                result = 0x0;
            }
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag41)
    revFlag41 = {
        void apply(inout bit<8> register_data) {
            register_data = 0x1;
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag42)
    setFlag42 = {
        void apply(inout bit<8> register_data, out bit<1> result) {
            if(register_data == 0x1){
                register_data = 0x0;
                result = 0x1;
            }else{
                result = 0x0;
            }
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag42)
    revFlag42 = {
        void apply(inout bit<8> register_data) {
            register_data = 0x1;
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag43)
    setFlag43 = {
        void apply(inout bit<8> register_data, out bit<1> result) {
            if(register_data == 0x1){
                register_data = 0x0;
                result = 0x1;
            }else{
                result = 0x0;
            }
        }
    };

    RegisterAction<bit<8>, _, bit<1>>(flag43)
    revFlag43 = {
        void apply(inout bit<8> register_data) {
            register_data = 0x1;
        }
    };

    Register<bit<16>, _>(32768) sketch11;
    Register<bit<16>, _>(32768) sketch12;
    Register<bit<16>, _>(32768) sketch13;

    Register<bit<16>, _>(32768) sketch21;
    Register<bit<16>, _>(32768) sketch22;
    Register<bit<16>, _>(32768) sketch23;

    Register<bit<16>, _>(32768) sketch31;
    Register<bit<16>, _>(32768) sketch32;
    Register<bit<16>, _>(32768) sketch33;

    Register<bit<16>, _>(32768) sketch41;
    Register<bit<16>, _>(32768) sketch42;
    Register<bit<16>, _>(32768) sketch43;

    RegisterAction<bit<16>, _, bit<1>>(sketch11)
    setValue11 = {
        void apply(inout bit<16> value) {
            value = value + 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch11)
    revValue11 = {
        void apply(inout bit<16> value) {
            value = 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch12)
    setValue12 = {
        void apply(inout bit<16> value) {
            value = value + 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch12)
    revValue12 = {
        void apply(inout bit<16> value) {
            value = 1;
        }
    };


    RegisterAction<bit<16>, _, bit<1>>(sketch13)
    setValue13 = {
        void apply(inout bit<16> value) {
            value = value + 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch13)
    revValue13 = {
        void apply(inout bit<16> value) {
            value = 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch21)
    setValue21 = {
        void apply(inout bit<16> value) {
            value = value + 1;
        }
    };
 
    RegisterAction<bit<16>, _, bit<1>>(sketch21)
    revValue21 = {
        void apply(inout bit<16> value) {
            value =1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch22)
    setValue22 = {
        void apply(inout bit<16> value) {
            value = value + 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch22)
    revValue22 = {
        void apply(inout bit<16> value) {
            value = 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch23)
    setValue23 = {
        void apply(inout bit<16> value) {
            value = value + 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch23)
    revValue23 = {
        void apply(inout bit<16> value) {
            value = 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch31)
    setValue31 = {
        void apply(inout bit<16> value) {
            value = value + 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch31)
    revValue31 = {
        void apply(inout bit<16> value) {
            value = 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch32)
    setValue32 = {
        void apply(inout bit<16> value) {
            value = value + 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch32)
    revValue32 = {
        void apply(inout bit<16> value) {
            value = 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch33)
    setValue33 = {
        void apply(inout bit<16> value) {
            value = value + 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch33)
    revValue33 = {
        void apply(inout bit<16> value) {
            value = 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch41)
    setValue41 = {
        void apply(inout bit<16> value) {
            value = value + 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch41)
    revValue41 = {
        void apply(inout bit<16> value) {
            value = 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch42)
    setValue42 = {
        void apply(inout bit<16> value) {
            value = value + 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch42)
    revValue42 = {
        void apply(inout bit<16> value) {
            value = 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch43)
    setValue43 = {
        void apply(inout bit<16> value) {
            value = value + 1;
        }
    };

    RegisterAction<bit<16>, _, bit<1>>(sketch43)
    revValue43 = {
        void apply(inout bit<16> value) {
            value = 1;
        }
    };

    apply {
        if (hdr.ipv4.isValid() && (hdr.tcp.isValid() || hdr.udp.isValid())) {
            do_hash1();
            do_hash2();
            do_hash3();
            simple_fwd.apply();
            bit<32> cycleN = getSub.execute(0);
            bit<15> index = (bit<15>)(cycleN & 0x7FFF);
            bit<2> subN = (bit<2>)(cycleN & 0x03);
            if(subN == 0x0){
                bit<1> flag1 = setFlag11.execute((bit<15>)meta.index_sketch1);
                if(flag1 == 0){
                    setValue11.execute((bit<15>)meta.index_sketch1);
                }else{
                    revValue11.execute((bit<15>)meta.index_sketch1);
                }
                bit<1> flag2 = setFlag12.execute((bit<15>)meta.index_sketch2);
                if(flag2 == 0){
                    setValue12.execute((bit<15>)meta.index_sketch2);
                }else{
                    revValue12.execute((bit<15>)meta.index_sketch2);
                }
                bit<1> flag3 = setFlag13.execute((bit<15>)meta.index_sketch3);
                if(flag3 == 0){
                    setValue13.execute((bit<15>)meta.index_sketch3);
                }else{
                    revValue13.execute((bit<15>)meta.index_sketch3);
                }
                revFlag21.execute(index);
                revFlag22.execute(index);
                revFlag23.execute(index);
            }else if(subN == 0x1){
                bit<1> flag1 = setFlag21.execute((bit<15>)meta.index_sketch1);
                if(flag1 == 0){
                    setValue21.execute((bit<15>)meta.index_sketch1);
                }else{
                    revValue21.execute((bit<15>)meta.index_sketch1);
                }
                bit<1> flag2 = setFlag22.execute((bit<15>)meta.index_sketch2);
                if(flag2 == 0){
                    setValue22.execute((bit<15>)meta.index_sketch2);
                }else{
                    revValue22.execute((bit<15>)meta.index_sketch2);
                }
                bit<1> flag3 = setFlag23.execute((bit<15>)meta.index_sketch3);
                if(flag3 == 0){
                    setValue23.execute((bit<15>)meta.index_sketch3);
                }else{
                    revValue23.execute((bit<15>)meta.index_sketch3);
                }
                revFlag31.execute(index);
                revFlag32.execute(index);
                revFlag33.execute(index);
            }else if(subN == 0x2){
                bit<1> flag1 = setFlag31.execute((bit<15>)meta.index_sketch1);
                if(flag1 == 0){
                    setValue31.execute((bit<15>)meta.index_sketch1);
                }else{
                    revValue31.execute((bit<15>)meta.index_sketch1);
                }
                bit<1> flag2 = setFlag32.execute((bit<15>)meta.index_sketch2);
                if(flag2 == 0){
                    setValue32.execute((bit<15>)meta.index_sketch2);
                }else{
                    revValue32.execute((bit<15>)meta.index_sketch2);
                }
                bit<1> flag3 = setFlag33.execute((bit<15>)meta.index_sketch3);
                if(flag3 == 0){
                    setValue33.execute((bit<15>)meta.index_sketch3);
                }else{
                    revValue33.execute((bit<15>)meta.index_sketch3);
                }
                revFlag41.execute(index);
                revFlag42.execute(index);
                revFlag43.execute(index);
            }else if(subN == 0x3){
                bit<1> flag1 = setFlag41.execute((bit<15>)meta.index_sketch1);
                if(flag1 == 0){
                    setValue41.execute((bit<15>)meta.index_sketch1);
                }else{
                    revValue41.execute((bit<15>)meta.index_sketch1);
                }
                bit<1> flag2 = setFlag42.execute((bit<15>)meta.index_sketch2);
                if(flag2 == 0){
                    setValue42.execute((bit<15>)meta.index_sketch2);
                }else{
                    revValue42.execute((bit<15>)meta.index_sketch2);
                }
                bit<1> flag3 = setFlag43.execute((bit<15>)meta.index_sketch3);
                if(flag3 == 0){
                    setValue43.execute((bit<15>)meta.index_sketch3);
                }else{
                    revValue43.execute((bit<15>)meta.index_sketch3);
                }
                revFlag11.execute(index);
                revFlag12.execute(index);
                revFlag13.execute(index);
            }
        }else{
            drop();
        }
    }
}

    /*********************  D E P A R S E R  ************************/

control IngressDeparser(
    packet_out pkt,
    /* User */
    inout my_ingress_headers_t hdr,
    in my_ingress_metadata_t meta,
    /* Intrinsic */
    in ingress_intrinsic_metadata_for_deparser_t ig_dprsr_md)
{
    apply {
        pkt.emit(hdr);
    }
}



struct my_egress_headers_t {
    ethernet_h   ethernet;
    ipv4_h       ipv4;
    l4port_h     ports;
    tcp_h        tcp;
    udp_h        udp;
}

struct my_egress_metadata_t {
}

/*************************************************************************
 ****************  E G R E S S   P R O C E S S I N G   *******************
 *************************************************************************/

    /***********************  P A R S E R  **************************/

parser EgressParser(
    packet_in pkt,
    /* User */
    out my_egress_headers_t hdr,
    out my_egress_metadata_t meta,
    /* Intrinsic */
    out egress_intrinsic_metadata_t eg_intr_md)
{
    /* This is a mandatory state, required by Tofino Architecture */
    state start {
        pkt.extract(eg_intr_md);
        transition meta_init;
    }

    state meta_init {
        transition parse_ethernet;
    }

    state parse_ethernet {
        pkt.extract(hdr.ethernet);
        transition select(hdr.ethernet.ether_type) {
            IPV4 :  parse_ipv4;
            default: reject;
        }
    }

    state parse_ipv4 {
        pkt.extract(hdr.ipv4);
        transition select(hdr.ipv4.frag_offset, hdr.ipv4.protocol) {
            ( 0, TCP  ) : parse_tcp;
            ( 0, UDP  ) : parse_udp;
            default : reject;
        }
    }

    state parse_tcp {
        pkt.extract(hdr.ports);
        pkt.extract(hdr.tcp);
        transition accept;
    }
    
    state parse_udp {
        pkt.extract(hdr.ports);
        pkt.extract(hdr.udp);
        transition accept;
    }
}

/*************************************************************************
 ****************  E G R E S S   P R O C E S S I N G   *******************
 *************************************************************************/

    /***************** M A T C H - A C T I O N  *********************/

control Egress(
    /* User */
    inout my_egress_headers_t hdr,
    inout my_egress_metadata_t meta,
    /* Intrinsic */    
    in egress_intrinsic_metadata_t eg_intr_md,
    in egress_intrinsic_metadata_from_parser_t eg_prsr_md,
    inout egress_intrinsic_metadata_for_deparser_t eg_dprsr_md,
    inout egress_intrinsic_metadata_for_output_port_t eg_oport_md)
{
    apply {

    }
}


    /*********************  D E P A R S E R  ************************/

control EgressDeparser(
    packet_out pkt,
    /* User */
    inout my_egress_headers_t hdr,
    in my_egress_metadata_t meta,
    /* Intrinsic */
    in egress_intrinsic_metadata_for_deparser_t eg_dprsr_md)
{
    apply {
        pkt.emit(hdr);
    }
}

/************ F I N A L   P A C K A G E ******************************/
Pipeline(
    IngressParser(),
    Ingress(),
    IngressDeparser(),
    EgressParser(),
    Egress(),
    EgressDeparser()
) pipe;

Switch(pipe) main;